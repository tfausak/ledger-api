name: ledger
version: 0.0.0
cabal-version: >=1.10
build-type: Simple
license: MIT
license-file: LICENSE.md
copyright: 2014 Taylor Fausak <taylor@fausak.me>
maintainer: Taylor Fausak <taylor@fausak.me>
homepage: https://github.com/tfausak/ledger
bug-reports: https://github.com/tfausak/ledger/issues
synopsis: TODO
description:
    TODO
category: Web
author: Taylor Fausak <taylor@fausak.me>
tested-with: GHC ==7.8.*
data-files:
    default.cfg
data-dir: data
extra-source-files:
    CHANGELOG.md
    CONTRIBUTING.md
    README.md
 
source-repository head
    type: git
    location: git://github.com/tfausak/ledger.git
 
library
    build-depends:
        base ==4.*,
        Cabal >=1.10,
        acid-state ==0.12.*,
        aeson ==0.8.*,
        configurator ==0.3.*,
        containers ==0.5.*,
        http-types ==0.8.*,
        mtl ==2.*,
        safecopy ==0.8.*,
        text ==1.*,
        time ==1.*,
        transformers ==0.3.*,
        wai ==3.*,
        warp ==3.*
    exposed-modules:
        Ledger
        Ledger.Actions
        Ledger.Actions.Entries
        Ledger.Application
        Ledger.Internal.Actions
        Ledger.Internal.Main
        Ledger.Main
        Ledger.Models
        Ledger.Models.Entry
        Ledger.Models.EntryRequest
        Ledger.Models.EntryResponse
        Ledger.Router
    exposed: True
    buildable: True
    default-language: Haskell2010
    hs-source-dirs: library
    other-modules:
        Paths_ledger
 
executable ledger
    build-depends:
        base -any,
        ledger -any
    main-is: Main.hs
    buildable: True
    default-language: Haskell2010
    hs-source-dirs: executable
 
 